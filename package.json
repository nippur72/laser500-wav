{
  "name": "laser500-wav",
  "version": "0.1.2",
  "description": "Laser500 program to WAV (tape) converter",
  "bin": {
    "laser500wav": "dist/laser500wav.js"
  },
  "scripts": {
    "build": "npx tsc --watch",
    "test0": "node dist/laser500wav -i test/mariel.bin -o test/mariel0 -n MARIEL0",
    "test1": "node dist/laser500wav -i test/mariel.bin -o test/mariel1 -n MARIEL1 -x --turbo-speed 1",
    "test2": "node dist/laser500wav -i test/mariel.bin -o test/mariel2 -n MARIEL2 -x --turbo-speed 2",
    "test3": "node dist/laser500wav -i test/mariel.bin -o test/mariel3 -n MARIEL3 -x --turbo-speed 3",
    "test4": "node dist/laser500wav -i test/mariel.bin -o test/mariel4 -n MARIEL4 -x --turbo-speed 4"
  },
  "keywords": [
    "laser500",
    "vtech",
    "videotechnology"
  ],
  "author": "Antonino Porcino <nino.porcino@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "fili": "^2.0.3",
    "wav-encoder": "^1.3.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.1",
    "@types/node": "^20.5.9",
    "@types/wav-encoder": "^1.3.0",
    "typescript": "^5.2.2"
  }
}
